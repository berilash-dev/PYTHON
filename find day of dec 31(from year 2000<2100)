import time
a=int(input("enter year: "))
b=a-2000
c=b//4
d=b-c
e=2*c+d
f=e%7
time.sleep(2)
match f:
    case 0:
        print("sunday")
    case 1:
        print("monday")
    case 2:
        print("tuesday")
    case 3:
        print("wednesday")
    case 4:
        print("thursday")
    case 5:
        print("friday")
    case 6:
        print("saturday")

